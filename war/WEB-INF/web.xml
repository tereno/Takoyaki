<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Oracle Corporation//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <servlet>
        <servlet-name>linkstash</servlet-name>
        <servlet-class>linkstash.LinkStashServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>linkstash</servlet-name>
        <url-pattern>/linkstash</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>stash</servlet-name>
        <servlet-class>linkstash.StashServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>stash</servlet-name>
        <url-pattern>/stash</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>linkstash.jsp</welcome-file>
    </welcome-file-list>
</web-app>
